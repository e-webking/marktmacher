{
    "name": "arm/armpackage",
    "type": "typo3-cms-extension",
    "description": "Furplan academy courses",
    "authors": [
        {
            "name": "Anisur Rahaman Mullick",
            "role": "Developer"
        }
    ],
    "require": {
        "typo3/cms-core": "^8.7.1"
    },
    "autoload": {
        "psr-4": {
            "ARM\\Armpackage\\": "Classes"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ARM\\Armpackage\\Tests\\": "Tests"
        }
    },
    "replace": {
        "armpackage": "self.version",
        "typo3-ter/armpackage": "self.version"
    }
}