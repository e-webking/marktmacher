    <el type="array">
        <field_hpos type="array">
        <TCEforms type="array">
          <label>Text Side</label>
          <config type="array">
            <type>select</type>
            <items type="array">
              <numIndex index="0" type="array">
                <numIndex index="0">Right</numIndex>
                 <numIndex index="1">ceright</numIndex>
              </numIndex>
              <numIndex index="1" type="array">
                <numIndex index="0">Left</numIndex>
                <numIndex index="1">celeft</numIndex>
              </numIndex>
            </items>
          </config>
        </TCEforms>
        </field_hpos>
        <field_baseheight type="array">
        <TCEforms type="array">
          <label>Base block height</label>
          <config type="array">
            <type>input</type>
            <size>10</size>
            <eval>int</eval>
            <default>370</default>
          </config>
        </TCEforms>
        </field_baseheight>
        <field_bottomheight type="array">
        <TCEforms type="array">
          <label>Bottom block height</label>
          <config type="array">
            <type>input</type>
            <size>10</size>
            <eval>int</eval>
            <default>170</default>
          </config>
        </TCEforms>
